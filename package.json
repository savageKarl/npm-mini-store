  "description": "基于 gulp4 + rollup2 搭建的微信小程序组件库打包脚手架",
  "scripts": {
    "devComponent": "cross-env NODE_ENV=dev gulp watch",
    "buildComponent": "cross-env NODE_ENV=pro gulp build",
    "devLibs": "cross-env NODE_ENV=dev rollup -c ./rollup/rollup.config.js -w",
    "buildLibs": "cross-env NODE_ENV=pro rollup -c ./rollup/rollup.config.js",
    "dev": "concurrently \"npm run devComponent\" \"npm run devLibs\" ",
    "build": "concurrently \"npm run buildComponent\" \"npm run buildLibs\" "
  },
  "files": [
    "/miniprogram_dist",
    "readme.md"
  ],
  "miniprogram": "miniprogram_dist",
